   -
     code : |1+
      PROCEDURE MaFenêtre()
      
      gsMotDePasse est une chaine // Mot de passe du fichier
   -
     code : |1+
      // Par défaut, le lecteur n'a pas de document ouvert : il faut donc griser les options de manipulation de document dans le menu
      BTN_MENU_IMPRIMER..etat = grise
      BTN_MENU_EXPORTER..etat = grise
      BTN_RACC_IMPRIMER..etat = grise
      // Masque également les informations de protection
      GR_Protection..visible = faux
      
      
      // Code spécifique à l'ouverture
      DocumentAction.OuvreDocument = __OuvrePDF
      
      	PROCÉDURE INTERNE __OuvrePDF( sChemin est une chaîne, xDocPos est un numérique = 0 ) : booléen
      	
      	// Demande si nécessaire le mot de passe du PDF
      	SI PDFDemandeMotDePasse(sChemin) = Faux ALORS renvoyer faux
      	
      	// Charge le fichier dans le lecteur PDF
      	si LecteurPDFOuvre(PDF_LecteurPDF, sChemin, gsMotDePasse) = faux alors renvoyer faux
      	
      	// Maintenant qu'un document est ouvert, active les options
      	BTN_MENU_IMPRIMER..Etat = actif
      	BTN_MENU_EXPORTER..Etat = Actif
      	BTN_RACC_IMPRIMER..etat = actif
      	// Masque également les informations de protection
      	GR_Protection..Visible = Vrai
      	
      	// Si les paramètres d'affichage doivent être restaurés
      	si COL_PARAMETRAGE_PDF.gbRestaureParamètresAffichages alors
      		// Récupère les informations supplémentaires spécifiques aux PDF
      		nModeAffichage, nZoom sont des entiers
      		(nModeAffichage, nZoom) = InformationsSupplémentairesRécupère( sChemin )
      		// Applique les informations (mode d'affichage, zoom)
      		si nModeAffichage <> -1 alors PDF_LecteurPDF..ModeAffichage = nModeAffichage
      		si nZoom <> -1 alors PDF_LecteurPDF..Zoom = nZoom
      	Fin
      	
      	// Si aucune position n'est demandée
      	si xDocPos <= 0 alors
      		// Vérifie dans la MRU si ce document a déjà été ouvert et récupère sa position
      		xDocPos = MenuRécupèreDernièrePosition(sChemin)
      	FIN
      	// Affiche la page mémorisée
      	si xDocPos > 0 ALORS
      		// Tant que cette page n'est pas encore accessible
      		tantque xDocPos > PDF_LecteurPDF..NombrePageAccessible
      			// Attend que le chargement du PDF avance
      			temporisation(100 ms, tempodessin)
      		FIN
      		// Affiche la page demandée
      		PDF_LecteurPDF..NuméroPageAffichée = xDocPos
      	FIN
      	
      	// Si le document comporte des signets, il faut les afficher
      	SignetDisponible()
      		
      	// Le fichier a bien été ouvert
      	renvoyer Vrai
      	
      	FIN
      
      // Par défaut, masque la zone de signets
      SignetAffiche(faux)
      
      // Si un document est passé en paramètre
      si lignecommande() <> "" ALORS
      	// Ouvre le document demandé
      	MenuOuvreDocument(remplace(LigneCommande(), """",""))
      FIN
      
      FIN:
      
      // Affiche le plan du lecteur par défaut
      MaFenêtre..Plan = PLAN_LECTEURPDF
      
      // Initialise le ruban
      oRuban est un champ <- PDF_LecteurPDF.ruban
      ProcedureInitRuban est une procédure = pMasquageRubanCallback
      InitialiseRuban(oRuban..nomcomplet, <pMasquageRubanCallback>:ProcedureInitRuban)
      
      	PROCÉDURE INTERNE pMasquageRubanCallback(oRubanNatif est un Champ)
      		
      		// Affiche le premier regroupement
      		sNomRegroupement est une chaîne = EnumèreChamp(oRubanNatif[1]..NomComplet, 1)
      		SI sNomRegroupement <> "" ALORS
      			{oRubanNatif..nomcomplet+"."+sNomRegroupement, indChamp}..Visible = Vrai
      		FIN
      		
      		// Masque le regroupement d'ouverture
      		sNomChamp est une chaîne = oRubanNatif..NomComplet + ".GRPRUB_WDFAA_General"
      		SI ChampExiste(sNomChamp) ALORS
      			oGroupeOuvrir est un Champ <- {sNomChamp, indChamp}
      			SI oGroupeOuvrir <> Null ALORS
      				oGroupeOuvrir..Visible = Faux
      			FIN
      		FIN
      		
      	FIN
      
