          type : 4
          code_elements :
           internal_properties : CAAAAAgAAACSlcBny5Ya3rZJ9jeJUTqhZWcNLFJJcHGjQdxuIpFfjA6UyPfAkfkgCsjzICB3HfRtBrCAMPSOl8V9TASuK3GKDrFD9g+apvKHgOr+ukf06NJC429Nz1LQnfWcbJclvkzXcw==
           type_code : 4
           p_codes :
            -
              code : |1-
               
               // Si on n'a pas encore enregistré l'association
               si ChargeParamètre("FileExtAssociation", "") = "" alors
               
               	// On devra mettre en place l'association
               	moimeme..Libellé = "Associer les fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] à [%ProjetInfo(piNomProjet)%]"
               	MoiMême..note    = "1"
               		
               sinon
               	
               	// On devra annuler l'association
               	MoiMême..Libellé = "Annuler l'association des fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] à [%ProjetInfo(piNomProjet)%]"
               	MoiMême..Note    = "0"
               fin
               	
               si enmodetest() = faux alors MoiMême..Bulle = MoiMême..Libellé
               moimeme..Largeur = textelargeur(moimeme, moimeme..libelle, tMonoligne) + CoordonnéeEditeurVersEcran(24 + 8)
               
               // Si l'UAC est actif et que l'utilisateur n'est pas administrateur
               si UACActif() _et_ UtilisateurEstAdministrateur() = faux ALORS
               	
               	MoiMême..Image = IMG_UAC
               FIN
              type : 14
            -
              internal_properties : CAAAAAgAAABfGQ6FITMFEiuwex5ACwhJx1H0AIhxs59bVldMpKH3n2WQRlQjrOFxXA//QFxqLQ7bLvKs4Dh0tvQrmi8mOshiGzNWhSNOwrIavFSu3KySGGnr2v1ghcAxps7XLjXTYVJD98FTMDwqPjCCo/g=
              code : |1-
               
               bAssociation est un booléen = Val(MoiMême..Note)
               
               // Associe la ou les extensions déclarées au programme
               si AssocieExtensions( bAssociation ) = faux ALORS
               	// Le lancement a été annulé
               	retour
               FIN
               
               si bAssociation alors
               	
               	// Marque que l'association a été effectuée
               	sauveParamètre("FileExtAssociation", "Done")
               	
               	// Affiche un toast
               	ToastAffiche("Les fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] s'ouvriront désormais avec [%ProjetInfo(piNomProjet)%]", toastCourt, cvMilieu)
               		
               sinon
               	
               	// Annule la marque que l'association a été effectuée
               	SauveParamètre("FileExtAssociation", "")
               	
               	// Affiche un toast
               	ToastAffiche("Les fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] ne s'ouvriront plus avec [%ProjetInfo(piNomProjet)%]", toastCourt, cvMilieu)
               fin
               
               // Mise à jour du bouton
               MoiMême.ExécuteTraitement( trtinit )
               
               // Ferme le menu
               MenuFichierFerme()
