     name : MenuOuvreDocument
     internal_properties : CAAAAAgAAACdXtf2n4sN74JZ13M5+j6lKQS7u/SPuT7UxPqB8tlIB5yTDbbiX7wRvLTTzszQ3hxtxtvKbZKFPt2MtBm9GziP4ZarUTWHBh9w3+LjYZiasit4eNopi/FEHV9jC7eM8Z8KwU4LnJQQsCWcN5PCaniy1qpWpLUElkv5YOnkJ18FayQOC+8aIUlNJ03NjLje99ZktEkP20QHa3an20nGtzfeJKZARW62FFL/C+OkgDaeA6NzbFRrqN0V7FaGHxgjhR2Ws7T+Kg==
     procedure_id : 1880217432466363764
     type_code : 14
     code : |1-
      // Résumé : Ouvre un document
      // Syntaxe :
      //MenuOuvreDocument (<sChemin> est chaîne [, <bRéutiliseFenêtre> est booléen [, <xDocPos> est numérique]])
      //
      // Paramètres :
      //	sChemin (chaîne ANSI) : Chemin complet du document
      //	bRéutiliseFenêtre (booléen - valeur par défaut=0) : Vrai si on doit recycler la fenêtre courante, Faux sinon
      //	xDocPos (numérique - valeur par défaut=0) : Position sauvegardée dans le document
      // Valeur de retour :
      // 	Aucune
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE MenuOuvreDocument(local sChemin est une chaine, bRéutiliseFenêtre est un booléen = faux, LOCAL xDocPos est un numerique = 0)
      			
      	sAlias est une chaine
      	bLectureSeule est un booléen = ( BTN_MENU_NOUVEAU..Visible = Faux _et_ BTN_MENU_ENREGISTRER..visible = faux )
      	bDepuisDropExplorateur est un booléen = ( _eve.wMessage = 0x0233 ) // WM_DROPFILES
      	bOuvertureNvelleFenêtre est un booléen = faux
      	
      	sablier( vrai )
      	
      	// Si on tente d'ouvrir le document actuellement ouvert
      	si sChemin ~= gsCheminDocument ALORS
      		
      		// Ferme le menu fichier
      		MenuFichierFerme( (PAS bOuvertureNvelleFenêtre) )
      				
      		// Abandonne
      		retour
      	FIN
      			
      	// Si aucun document n'est ouvert et que la création n'est pas active / ou si on a dropé un fichier au lancement / si on a crée un document sans le modifier
      	si bRéutiliseFenêtre _ou_ ( bDepuisDropExplorateur _et_ gsCheminDocument = "" ) _ou_ ( gsCheminDocument = "" _et_ bLectureSeule ) _ou_ ( gsCheminDocument = "" _et_ gbDocumentModifié = faux ) ALORS
      				
      		// On ouvre en lieu et place
      		sAlias = gFenDocument..alias = "" ? gFenDocument..nom sinon gFenDocument..alias
      		
      		// Si on a dropé un fichier de l'explorateur ou si on recycle la fenêtre
      		si bRéutiliseFenêtre _ou_ bDepuisDropExplorateur ALORS
      			
      			// Si le document en cours n'a pas été sauvegardé
      			si MenuFermeDocument( faux ) = faux ALORS
      				
      				// Abandon
      				retour
      			FIN
      		FIN		
      	sinon
      		
      		sAlias = "DOC_" + dateheuresys		
      		
      		// Ouvre un alias de la fenêtre
      		OuvreSoeur(sAlias + " = " + gFenDocument..nom + SansFocus)
      		
      		// Maximise la fenêtre si on est maximisé
      		SI FenTaille(gFenDocument) = TailleMaxi ALORS FenTaille(sAlias, TailleMaxi)
      		
      		// On mémorise le fait qu'on ouvre une autre fenêtre
      		bOuvertureNvelleFenêtre = vrai
      	FIN
      			
      	// Prend une référence sur les actions de l'alias
      	pstDocumentActions est une action dynamique = {sAlias + ".DocumentAction", indvariable}
      				
      	// Ouvre le fichier
      	si pstDocumentActions.OuvreDocument = null _ou_ pstDocumentActions.OuvreDocument( sChemin, xDocPos ) = vrai ALORS
      		
      		// Ajoute le fichier à liste des documents récents
      		SysDocRécentAjoute( sChemin )
      		
      		// Mémorise le document dans la MRU
      		MRUAjoute( sChemin )
      		
      		// Charge les informations du fichier
      		ExécuteTraitement(sAlias + ".MenuAfficheInfos", trtprocedure, ( sChemin ))
      			
      		// Donne le focus à la fenêtre
      		sysDonneFocus( handle(sAlias) )
      	FIN
      		
      	// Ferme le menu fichier
      	MenuFichierFerme( (pas bOuvertureNvelleFenêtre) )
      	
      CAS EXCEPTION:
      	dbgAssertion( Faux, ExceptionInfo )
      	retour
      	
      fin:
      	
      	sablier( faux )
      
      	si bDepuisDropExplorateur ALORS
      		_eve.retour = faux
      	FIN
     type : 458752
   -
     name : MenuAfficheInfos
     procedure_id : 1880217432466429300
     type_code : 14
     code : |1-
      PROCÉDURE MenuAfficheInfos(LOCAL sChemin est une chaîne)
      
      	//Exécute le traitement défini dans le modèle
      	ExécuteAncêtre(sChemin)
      	
      	// Si le chemin n'est pas valide
      	si sChemin ~= "" ALORS
      		
      		// Libellé par défaut
      		LIB_INFO_PROTECTIONFICHIER = "Aucun fichier ouvert"
      		// Le PDF est en lecture uniquement : si aucun document n'est ouvert, il faut modifier les libellés du modèle
      		LIB_INFO_DATECREATION = "Aucun fichier ouvert"
      		LIB_INFO_DATEMODIFICATION = "Aucun fichier ouvert"
      		LIB_INFO_TAILLE = "Aucun fichier ouvert"
      		
      	SINON
      		
      		// Si le fichier est protégé (en lecture)
      		si PDFEstProtégé(sChemin) ALORS
      			LIB_INFO_PROTECTIONFICHIER = "Le fichier est protégé par mot de passe (lecture)"
      		SINON // Pas de mot de passe
      			LIB_INFO_PROTECTIONFICHIER = "Le fichier n'est pas protégé par mot de passe (lecture)"
      		FIN
      		
      	FIN
      	
      	// Si le fichier est protégé
      	soit bFichierProtégé = (ffichierexiste(gsCheminDocument) _et_ PDFEstProtégé(gsCheminDocument))
      	
      	// Mise à jour du titre de la fenêtre (ajoute l'indication de PDF protégé)
      	MaFenêtre..Titre = ( gsNomDocumentOuvert + (gbDocumentModifié ? "*" SINON "") + (bFichierProtégé ? " [PROTÉGÉ]" SINON "") )  + [" - "] + SansEspace(ExtraitChaîne(ProjetInfo(piNomProjet), 2, "WD Bureautique "))
     type : 458752
     template_refs :
      - 1532303357569291946
