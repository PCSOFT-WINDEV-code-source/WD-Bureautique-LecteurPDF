      	BTN_MENU_EXPORTER..ImageFond = sImageFond
      	BTN_MENU_IMPRIMER..ImageFond = sImageFond
      	BTN_MENU_ENREGISTRER_SOUS..ImageFond = sImageFond
      	BTN_MENU_OUVRIR..ImageFond = sImageFond
      	BTN_MENU_NOUVEAU..ImageFond = sImageFond
