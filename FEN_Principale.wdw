     name : OuvreSélecteurFichier
     internal_properties : CAAAAAgAAACGGhBKasTs3BkJhWOtHhgnaeRBuo2L8Qc79IGlTSs9ieQTNew024+q7QjRGEKn9i/x6ddG2HAN0Tl949xDQr0FYP3+/lcNye7o525fJkINogyIr6wb4BWbRvgfXRuW7iTKqWU=
     procedure_id : 1880321941876957213
     type_code : 14
     code : |1-
      // Résumé : Ouvre le sélecteur de fichiers
      // Syntaxe :
      //[ <Résultat> = ] OuvreSélecteurFichier ( [<nMode> est entier [, <sRépertoireInitial> [, <sFiltre> est chaîne [, <sExtensionDéfaut> est chaîne]]]])
      //
      // Paramètres :
      //	nMode (entier - valeur par défaut=2) : Mode du sélecteur (constantes fSelXXX)
      //	sRépertoireInitial (valeur par défaut="") : Répertoire d'ouverture (par défaut dernier répertoire ouvert ou répertoire du document courant)
      //	sFiltre (chaîne ANSI - valeur par défaut="") : Filtre du sélecteur de fichiers
      // 	sExtensionDéfaut (chaîne ANSI - valeur par défaut="") : Extension par défaut (sans le *.)
      // Valeur de retour :
      // 	chaîne ANSI : Chemin sélectionné
      //
      PROCEDURE OuvreSélecteurFichier(nMode est un entier = fselOuvre, local sRépertoireInitial = "", local sFiltre est une chaine = "", local sExtensionDéfaut est une chaine = "") : chaine
      	
      	sFichier est une chaine
      	sRépertoireInitial = (sRépertoireInitial <> "" ? sRépertoireInitial sinon ChargeParamètre("Recent\LastPath", ( gsCheminDocument = "" ? sysrep( srMesDocuments ) sinon fExtraitChemin(gsCheminDocument, fDisque + fRépertoire) )))
      	
      	// Si on n'a pas fourni de filtre par défaut
      	si sFiltre = "" ALORS
      		sFiltre = "[%COL_PARAMETRAGE.DESCRIPTION_DOCUMENT%] ([%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%])" + TAB + COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR
      	FIN
      	
      	// Si on n'a pas fourni d'extension par défaut
      	si sExtensionDéfaut = "" alors
      		sExtensionDéfaut = extraitchaine(remplace( COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR , "*.", "" ), 1, ";")
      	fin
      		
      	// Ouvre le sélecteur de fichiers
      	sFichier = fSélecteur(sRépertoireInitial, gsNomDocumentOuvert, (nMode & fselCrée = fselcree ? "Enregistrer sous" sinon "Ouvrir"), "Tous les fichiers (*.*)" + TAB + "*.*" + RC + ...
      											  sFiltre, sExtensionDéfaut, nMode )
      	
      	// Si aucun fichier n'a été sélectionné
      	SI sFichier = "" ALORS renvoyer ""
      	
      	// Mémorise le chemin
      	SauveParamètre("Recent\LastPath", fExtraitChemin(sFichier, fDisque + fRépertoire))
      	
      renvoyer sFichier
