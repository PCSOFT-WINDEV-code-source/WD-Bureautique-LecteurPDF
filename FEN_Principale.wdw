     name : PDFDemandeMotDePasse
     internal_properties : CAAAAAgAAAAWYRJkzkpSGQjoR3mmJCvO0aYWCv4xNRbSNBxBucIigwzBa/I5fIChEkrGJdYSZdpwQDxlrkLZ8+wYyV6E0MQZAzxaisR2g7pqEs7yvHVezU6jiQVN1qOwTCEMDgPMVnczgpEjxBnlnpDrOQILCwbrlOJo2eeoa5BLiJerwGAINQUv17UGkZDtm/Ay4qfLnj5sIDsy7eY=
     procedure_id : 1880323543910224306
     type_code : 14
     code : |1-
      // Résumé : Demande le mot de passe du PDF à l'utilisateur si nécessaire. Le mot de passe est mémorisé.
      // Syntaxe :
      //[ <Résultat> = ] PDFDemandeMotDePasse (<sFichierPDF> est chaîne)
      //
      // Paramètres :
      //	sFichierPDF (chaîne ANSI) : Chemin du fichier PDF
      // Valeur de retour :
      // 	booléen : Vrai pour poursuivre l'ouverture du PDF, Faux pour annuler (si le mot de passe correct n'a pas été saisi)
      //
      PROCEDURE PDFDemandeMotDePasse(LOCAL sFichierPDF est une chaine) : booléen
      
      // Si le fichier est protégé en lecture par un mot de passe
      SI PDFEstProtégé(sFichierPDF) ALORS
      	
      	// Si un mot de passe est déjà défini, ne rien faire (il a déjà été saisi)
      	si gsMotDePasse <> "" ALORS renvoyer vrai
      	
      	// Demande la saisie du mot de passe
      	si SaisitMotDePasse() = faux alors renvoyer faux
      	
      		PROCÉDURE INTERNE SaisitMotDePasse()
      		
      		// Demande à l'utilisateur de saisir le mot de passe
      		gsMotDePasse = Ouvre(FEN_MotDepasse)
      		
      		// Si aucun mot de passe n'est saisi, annule l'ouverture
      		SI gsMotDePasse ~= "" ALORS renvoyer faux
      		
      		// Pour vérifier le mot de passe, extrait le texte de la page 1 du PDF
      		PDFVersTexte(sFichierPDF, "1", gsMotDePasse)
      		// Si une erreur est détectée et qu'il s'agit d'une erreur de mot de passe
      		SI ErreurDétectée _ET_ ErreurInfo(errCode) = 370011 ALORS
      			
      			// Affiche un message à l'utilisateur
      			Info("Le mot de passe est erroné.", "Vérifiez si la touche Verr. maj n'est pas activée par erreur, puis recommencez.")
      			
      			// Redemande à l'utilisateur la saisie du mot de passe
      			si SaisitMotDePasse() = faux alors renvoyer faux
      			
      		FIN
      		
      		renvoyer vrai
      		
      		FIN
      	
      FIN
      
      
      // Si le PDF n'est pas protégé ou si le mot de passe fournit est correct, poursuit l'ouverture
      renvoyer vrai
