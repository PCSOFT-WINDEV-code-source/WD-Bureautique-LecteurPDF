     name : InitialiseRuban
     internal_properties : 
     procedure_id : 1880217432466167141
     type_code : 14
     starred : true
     code : |1-
      // Résumé : Initialise le ruban personnalisé (obligatoire après le code d'initialisation de la fenêtre)
      // Syntaxe :
      //InitialiseRuban (<sRubanSource> est chaîne [, <oRubanPersonnalisé> est champ [, <pMasquageRubanCallback> est procédure]])
      //
      // Paramètres :
      //	sRubanSource (chaîne ANSI) : Nom du ruban natif
      //	oRubanPersonnalisé (champ - valeur par défaut=0) : Champ ruban personnalisé
      // 	pMasquageRubanCallback (procédure - valeur par défaut=0) : Callback personnalisée de masquage du ruban
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE InitialiseRuban(sRubanSource est une chaine, oRubanPersonnalisé est un champ = null, pMasquageRubanCallback est une procédure = null)
      
      	// Rend la fenêtre visible
      	gFenDocument..visible = vrai
      
      	// Vérifie l'existence du champ du ruban source
      	SI ChampExiste(sRubanSource) = faux ALORS
      		
      		// Il n'existe pas : il a été personnalisé ou il s'agit de la barre d'outils standard, on ignore
      		retour
      	FIN
      		
      	oRubanNatif est un champ <- {sRubanSource, indChamp}
      	
      	// Applique le même style que la fenêtre
      	oRubanNatif..Image 	= gsImageVoletRuban
      	oRubanNatif..Style 	= RUBAN_STYLE..Style
      	oRubanNatif..Image 	= gsImageVoletRuban
      		
      	// Masque le premier regroupement du ruban (ses fonctionnalités correspondent au menu fichier géré par le modèle)
      	sNomRegroupement est une chaine = EnumèreChamp(oRubanNatif[1]..nomcomplet, 1)
      	si sNomRegroupement <> "" alors {oRubanNatif.nomcomplet+"."+sNomRegroupement, indchamp}..visible = faux
      	
      	// Si on a fournit une callback personnalisée pour le masquage du ruban
      	si pMasquageRubanCallback <> null alors
      		// Appelle la callback personnalisée de masquage du ruban
      		pMasquageRubanCallback( oRubanNatif )
      	fin
      	
      	// Change la couleur de fond
      	oCouleur est une couleur = LIB_FOND_BARRE_TITRE..CouleurFond
      	oRubanNatif..CouleurFond = oCouleur
      	
      	// Génère l'image de fond 5 états pour les boutons
      	avec oCouleur
      	
      		oImageBoutonStyle est une image
      		oImageBoutonStyle..Largeur = 100
      		oImageBoutonStyle..hauteur = 20
      		
      		// Début du dessin
      		dDébutDessin(oImageBoutonStyle, dAvecOpacité)
      		
      		// Enfoncé
      		oCouleur = LIB_FOND_BARRE_TITRE..CouleurFond
      		..Saturation = 180
      		..Luminosité = 95
      		..Opacité = 50
      		
      		dfond(..Couleur, FondPlein, 0, ..Opacité)
      		dStylo(Transparent)
      		drectangle(20, 0, 39, 19)
      
      		// Actif
      		oCouleur = LIB_FOND_BARRE_TITRE..CouleurFond
      		..Saturation = 180
      		..Luminosité = 115
      		..Opacité = 50
      		
      		dfond(..Couleur, FondPlein, 0, ..Opacité)
      		dStylo(Transparent)
      		drectangle(60, 0, 79, 19)
      		
      		// Survol
      		oCouleur = LIB_FOND_BARRE_TITRE..CouleurFond
      		..Saturation = 30
      		..Luminosité = 160
      		..Opacité = 40
      		
      		dfond(..Couleur, FondPlein, 0, ..Opacité)
      		dStylo(transparent)
      		drectangle(80, 0, 99, 19)		
      		
      		// Sauvegarde
      		gsImageBoutonStyle = fRepDonnéesUtilisateur() + [fsep] + "btn5.png"
      		dSauveImagePNG(oImageBoutonStyle, gsImageBoutonStyle)
      	fin
      	
      	// Applique l'image générée sur les boutons déclarés
      	pour tout oChamp de gtabChampsStyle
      		si oChamp = null alors continuer
      			
      		// Applique le style
      		__AppliqueStyle(oChamp)
      	FIN
      			
      	// Insère les volets personnalisés dans le ruban natif : permet de conserver les mises à jour du ruban "natif"
      	si oRubanPersonnalisé <> null alors
      		RubanDupliqueVolets(oRubanPersonnalisé, oRubanNatif)	
      	fin
      		
      	// Déplace les boutons d'UNDO/REDO
      	DéplaceUndoRedo()
      	
      	// Déplace le libellé de lecture seule
      	DéplaceLectureSeule()
      	
      fin:	
      	// Le ruban est mis à jour toutes les 250ms
      	TimerSys(SurMiseAJourRuban, 250ms)
     type : 458752
   -
